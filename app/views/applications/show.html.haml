%p#notice= notice
%h1= @application.name
.row
  .small-12.large-12.columns
    %p Categories: #{@application.categories.map { |c| c.name }.join(', ')}
    %p Key: #{@application.key}
= link_to '<i class="foundicon-edit"></i> Edit'.html_safe, edit_application_path(@application), class: 'main-button'
